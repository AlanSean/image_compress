{
  "name": "image_compress",
  "version": "0.0.1",
  "description": "png/jpge optimization app",
  "homepage": "https://github.com/AlanSean/image_compress",
  "author": {
    "name": "AlanSean",
    "email": "miaomimiya@gmail.com"
  },
  "keywords": [
    "angular 11",
    "electron",
    "nodejs",
    "typescript",
    "spectron",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux"
  ],
  "resolutions": {
    "electron-builder/**/app-builder-bin": "3.5.13"
  },
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "dev": "npm-run-all -p electron:serve ng:dev",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:dev": "ng serve --hmr -c dev",
    "ng:serve": "ng serve -c web",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:pack": "npm run build:prod && electron-builder --dir",
    "electron:build": "npm run build:prod && npm run electron:serve-tsc && electron-builder build",
    "debug": "npm run electron:serve-tsc && electron-builder build",
    "release": "electron-builder  --publish always",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "pretty-quick": "pretty-quick --staged",
    "prettier": "prettier -c --write '**/*'",
    "prepare": "husky install"
  },
  "dependencies": {
    "crypto-js": "4.0.0",
    "execa": "5.0.0",
    "fs-extra": "9.1.0",
    "sharp": "^0.28.1"
  },
