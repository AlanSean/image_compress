{
  "name": "image_compress",
  "version": "0.0.2",
  "description": "WEBP,PNG/JPEG compression graphics program that base on Electron，Angular，Node，Sharp. Available for Windows, MacOS and Linux",
  "homepage": "https://github.com/AlanSean/image_compress",
  "bugs": "https://github.com/AlanSean/image_compress/issues",
  "author": {
    "name": "AlanSean",
    "email": "miaomimiya@gmail.com"
  },
  "keywords": [
    "angular 11",
    "electron",
    "nodejs",
    "typescript",
    "spectron",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux"
  ],
  "main": "dist/main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng:dev": "ng serve --hmr -c dev",
    "ng:build": "ng build --base-href ./",
    "dev": "npm-run-all -p ng:dev  electron:serve ",
    "build": "npm-run-all -p build:prod electron:tsc",
    "build:dev": "npm run ng:build -- -c dev",
    "build:prod": "npm run  ng:build  -- -c production",
    "electron:tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && cross-env CURRENT_ENV=serve etan start -w",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:pack": "npm run build:prod && etan build --dir",
    "electron:build": "npm run build && etan build",
    "debug": "etan build",
    "release": "etan build --publish always",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "npm run lint:ts",
    "lint:ts": "ng lint --fix",
    "prepare": "husky install",
    "help": "etan build --publish always"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "dependencies": {
    "@etanjs/node-find-files": "1.0.6",
    "crypto-js": "4.0.0",
    "dotenv": "9.0.2",
    "electron-log": "4.4.1",
    "electron-updater": "4.3.9",
    "execa": "5.0.0",
    "fs-extra": "9.1.0",
    "sharp": "0.29.1",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "13.0.0",
    "@angular-devkit/build-angular": "^12.2.14",
    "@angular-eslint/builder": "^12.7.0",
    "@angular-eslint/eslint-plugin": "^12.7.0",
    "@angular-eslint/eslint-plugin-template": "^12.7.0",
    "@angular-eslint/schematics": "^12.7.0",
    "@angular-eslint/template-parser": "^12.7.0",
    "@angular/animations": "12.2.14",
    "@angular/cli": "12.2.14",
    "@angular/common": "12.2.14",
    "@angular/compiler": "12.2.14",
    "@angular/compiler-cli": "12.2.14",
    "@angular/core": "12.2.14",
    "@angular/forms": "12.2.14",
    "@angular/language-service": "12.2.14",
    "@angular/platform-browser": "12.2.14",
    "@angular/platform-browser-dynamic": "12.2.14",
    "@angular/router": "12.2.14",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@etanjs/cli": "1.0.3",
    "@ngrx/effects": "11.1.1",
    "@ngrx/store": "11.1.1",
    "@ngrx/store-devtools": "11.1.1",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/ansi-escapes": "4.0.0",
    "@types/chai": "4.3.0",
    "@types/chalk": "2.2.0",
    "@types/crypto-js": "4.0.1",
    "@types/fs-extra": "9.0.11",
    "@types/imagemin": "7.0.0",
    "@types/jasmine": "3.6.9",
