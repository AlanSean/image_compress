{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-electron": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/renderer",
            "index": "src/renderer/index.html",
            "main": "src/renderer/main.ts",
            "tsConfig": "src/renderer/tsconfig.app.json",
            "polyfills": "src/renderer/polyfills.ts",
            "allowedCommonJsDependencies": ["fs-extra", "crypto-js/md5"],
            "aot": true,
            "assets": ["src/assets"],
            "styles": ["node_modules/ng-zorro-antd/style/index.min.css", "src/renderer/styles.less"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "./angular.webpack.js"
            }
          },
          "configurations": {
            "dev": {
              "optimization": false,
              "sourceMap": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "src/renderer/environments/environment.ts",
                  "with": "src/renderer/environments/environment.dev.ts"
                }
              ]
            },
            "web": {
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "src/renderer/environments/environment.ts",
                  "with": "src/renderer/environments/environment.web.ts"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/renderer/environments/environment.ts",
                  "with": "src/renderer/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "angular-electron:build"
          },
          "customWebpackConfig": {
            "path": "./angular.webpack.js"
          },
          "configurations": {
            "dev": {
              "browserTarget": "angular-electron:build:dev"
            },
            "web": {
              "browserTarget": "angular-electron:build:web"
            },
            "production": {
              "browserTarget": "angular-electron:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-electron:build"
          }
        },
        "test": {
          "builder": "@angular-builders/custom-webpack:karma",
          "options": {
            "main": "src/renderer/test.ts",
            "polyfills": "src/renderer/polyfills-test.ts",
            "tsConfig": "src/renderer/tsconfig.spec.json",
            "karmaConfig": "src/renderer/karma.conf.js",
            "scripts": [],
            "styles": ["src/renderer/styles.less"],
            "assets": ["src/assets"],
            "customWebpackConfig": {
              "path": "./angular.webpack.js"
            },
            "fileReplacements": [
              {
                "replace": "src/renderer/environments/environment.ts",
                "with": "src/renderer/environments/environment.test.ts"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": ".eslintrc.json",
            "lintFilePatterns": ["src/renderer/**.ts", "main.ts"]
          }
        }
      }
    },
    "angular-electron-e2e": {
      "root": "e2e",
      "projectType": "application",
      "architect": {
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "eslintConfig": ".eslintrc.json",
            "lintFilePatterns": ["e2e/**.ts"]
          }
        }
      }
    }
  },
  "defaultProject": "angular-electron",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "less"
    },
    "@schematics/angular:application": {
      "strict": true
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
